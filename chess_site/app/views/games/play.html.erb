<h1 style="text-align:center"><%= 'Game Over' if @game.end_time %></h1>
<div class="chessboard">
	<% (1..8).each do |row| %>
		<% (1..8).each do |col| %>
			<div class="tile <%= ((row+col)%2==0) ? 'white' : 'black' %>" id="<%= row.to_s+col.to_s%>">&nbsp;<span class="piece" piece="<%= @cboard[(row-1)*8+col-1] %>"><%= raw(@handle.html_piece @cboard[(row-1)*8+col-1])%></span>&nbsp;</div>
		<% end %>
		<br>
	<% end %>
</div>